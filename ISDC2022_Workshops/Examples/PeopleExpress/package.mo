within ISDC2022_Workshops.Examples;

package PeopleExpress "Simplified model of boom and bust dynamics for PEX"
  extends BusinessSimulation.Icons.Package;
  annotation(Documentation(info = "<html>
<p class=\"aside\">This information is part of the ISDC2022 Workshops package.</p>
<p>This package shows the development of a larger model from subsystem models losely following the stylized example given by John Morecroft [<a href=\"modelica://ISDC2022_Workshops.UsersGuide.References\">1</a>] [<a href=\"modelica://ISDC2022_Workshops.UsersGuide.References\">2</a>], which tries to give an endogenous explanation for the spectacular demise of People Express, a US low-cost airline.</p>
</html>"), Icon(coordinateSystem(extent = {{-100, -100}, {100, 100}}, preserveAspectRatio = true, initialScale = 0.1, grid = {10, 10}), graphics = {Text(visible = true, textColor = {255, 255, 255}, extent = {{-87.106, -30}, {87.106, 30}}, textString = "PEX", fontName = "Lato Black", textStyle = {TextStyle.Bold})}));
end PeopleExpress;
